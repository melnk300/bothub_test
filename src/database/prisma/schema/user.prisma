enum UserRole {
  USER
  ADMIN
}

model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  name     String
  avatar   String
  password String
  role     UserRole @default(USER)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  feedbacks Feedback[]
  tokens    Token[]
}
